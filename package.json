{
  "name": "fjx",
  "version": "0.0.1-rc2",
  "description": "functional javascript extension",
  "main": "dist/fjx.js",
  "module": "dist/fjx.esm.js",
  "sideEffects": false,
  "types": "declaration/index.d.ts",
  "keywords": [
    "functional",
    "javascript",
    "typescript",
    "fp",
    "fx"
  ],
  "scripts": {
    "test": "jest --watchAll",
    "coverage": "jest --coverage && print-coveralls --sort=desc",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "doc": "rm -rf ./doc && jsdoc -c jsdoc.json",
    "build": "npm run build:rollup && npm run declaration && npm run release:latest",
    "declaration": "rm -rf declaration && tsc -p tsconfig.declaration.json",
    "build:rollup": "rollup -c && print-sizes ./dist",
    "deploy": "gh-pages -d ./demo --dest=./ --add",
    "release": "npm run build && npm run release:current && npm run release:doc && npm run deploy",
    "release:mkdir": "mkdir -p ./demo/release",
    "release:doc": "npm run doc && cp -a ./doc/. ./demo/release/$npm_package_version/doc &&  cp -a ./doc/. ./demo/release/latest/doc",
    "release:current": "npm run release:mkdir && rm -rf ./demo/release/$npm_package_version && cp -a ./dist/. ./demo/release/$npm_package_version",
    "release:latest": "npm run release:mkdir && rm -rf ./demo/release/latest && cp -a ./dist/. ./demo/release/latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daybrush/fjx.git"
  },
  "author": "Daybrush",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daybrush/fjx/issues"
  },
  "homepage": "https://github.com/daybrush/fjx",
  "devDependencies": {
    "@daybrush/jsdoc": "^0.2.1",
    "@types/jest": "^23.3.10",
    "coveralls": "^3.0.2",
    "daybrush-jsdoc-template": "^1.3.2",
    "gh-pages": "^2.0.1",
    "jest": "^23.6.0",
    "print-coveralls": "^1.2.1",
    "print-sizes": "0.0.3",
    "rollup": "^0.67.4",
    "rollup-plugin-es3": "^1.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-prototype-minify": "^1.0.4",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-typescript": "^1.0.0",
    "rollup-plugin-uglify": "^6.0.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "@daybrush/utils": "^0.4.0"
  }
}
